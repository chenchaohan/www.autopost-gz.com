			$(this).find('dd').css('display', 'none');
		});
	//登录注册按钮
		$('.j-register').live('click',function() {
			$('.register-box').css('display', 'block');
			$(".mask").css("display","block");
			$(".mask-click").css("display","block");
			$(".login-box").css("display","none");
			return false;
		});

		$('.j-login').live('click',function() {
			$('.login-box').css('display', 'block');
			$(".mask").css("display","block");
			$(".mask-click").css("display","block");
			$(".register-box").css("display","none");
			return false;
		});
//表单验证注册
		$('.j-submit-register').click(function() {
			var tip = $('.tips');
			var username = $('#register-username');
			var email = $('#email');
			var password = $('#password');
			var repassword = $('#repassword');
			var code = $('#register-code');

			if(username.val() == '') {
				username.focus();
				tip.html('请输入用户名').fadeIn();
				return false;
			} else if(username.val().length < 3 || username.val().length > 16) {
				username.focus();
				tip.html('用户名长度必须在3-16个字符之间').fadeIn();
				return false;
			} else if(email.val() == '') {
				email.focus();
				tip.html('请输入邮箱').fadeIn();
				return false;
			} else if(!/^\w+((-\w+)|(\.\w+))*\@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z0-9]+$/.test(email.val())) {
				email.focus();
				tip.html('邮箱格式不对').fadeIn();
				return false;
			} else if(password.val() == '') {
				password.focus();
				tip.html('请输入密码').fadeIn();
				return false;
			} else if(!/^[^\s]{6,16}$/.test(password.val())) {
				password.focus();
				tip.html('密码区分大小写，由6-16个英文字母和数字组成').fadeIn();
				return false;
			} else if(repassword.val() == '') {
				repassword.focus();
				tip.html('密码不一致').fadeIn();
				return false;
			} else if(password.val() != repassword.val()) {
				repassword.focus();
				tip.html('密码不一致').fadeIn();
				return false;
			} else if(code.val() == '') {
				code.focus();
				tip.html('请输入验证码').fadeIn();
				return false;
			} else if(code.val().length != 5) {
				code.focus();
				tip.html('验证码长度只能为5位').fadeIn();
				return false;
			}

			tip.fadeOut();
		});
//表单验证登录
		$('.j-submit-login').click(function() {
			var tip = $('.tips');
			var username = $('#username');
			var password = $('#login-password');
			var code = $('#login-code');

			if(username.val() == '') {
				username.focus();
				tip.html('请输入用户名').fadeIn();
				return false;
			} else if(password.val() == '') {
				password.focus();
				tip.html('请输入密码').fadeIn();
				return false;
			} else if(!/^[^\s]{6,16}$/.test(password.val())) {
				password.focus();
				tip.html('密码区分大小写，由6-16个英文字母和数字组成').fadeIn();
				return false;
			} else if(code.val() == '') {
				code.focus();
				tip.html('请输入验证码').fadeIn();
				return false;
			} else if(code.val().length != 5) {
				code.focus();
				tip.html('验证码长度只能为5位').fadeIn();
				return false;
			}

			tip.fadeOut();
		});

		$('.j-login-switch').click(function(){
			$(".register-box").css("display","none");
			$(".login-box").css("display","block");
		});

		$('.j-register-switch').click(function(){
			$(".login-box").css("display","none");
			$(".register-box").css("display","block");
		});

		$('.mask-click').click(function(){
			$(".register-box").css("display","none");
			$(".login-box").css("display","none");
			$(".mask").css("display","none");
			$(".mask-click").css("display","none");
		});
		$('.mask').click(function(){
			$(".register-box").css("display","none");
			$(".login-box").css("display","none");
			$(".mask").css("display","none");
			$(".mask-click").css("display","none");
		});
	});

